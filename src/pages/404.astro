---
import Error404 from "@/components/infopages/Error404.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "404 Error: Page Not Found - Hermosa Software",
	description:
		"Oops! The page you're looking for doesn't exist. Navigate back to Hermosa Software's homepage for our full range of Web and Mobile Application Development, SEO and other services.",
	canonical: "https://hermosasoftware.io/404/",
};
---

<BaseLayout seo={seo}>
	<Error404 />
</BaseLayout>
